{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/PyRFC/bin/Debug/net8.0/PyRFC.dll",
            "args": [],
            "cwd": "${workspaceFolder}/PyRFC",
            "console": "externalTerminal",
            "stopAtEntry": true
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}